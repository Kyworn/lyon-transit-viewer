# TCL API Token (Base64 encoded)
# Format: email:password encoded in base64
# Example: echo -n "your_email@example.com:your_password" | base64
TCL_API_TOKEN=your_base64_encoded_token_here

# Database Configuration
POSTGRES_USER=user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=lyon_transit

# Redis Configuration
REDIS_URL=redis://redis:6379

# Database URL for backend
DATABASE_URL=postgres://user:your_secure_password_here@db:5432/lyon_transit
